// Simple test demonstrating Vec::pop usage
// Shows the pattern: get last, then pop

alloc fn main(): void {
  (printf "=== Simple Vec::pop test ===\n")

  // Create a vector and add some elements
  (:= v (Vec::new<i32>))
  (:=< v (Vec::push v 100))
  (:=< v (Vec::push v 200))
  (:=< v (Vec::push v 300))

  (printf "Initial vec: size=%d\n" (Vec::len v))
  (printf "Elements: %d, %d, %d\n"
    (Vec::get v 0)
    (Vec::get v 1)
    (Vec::get v 2))

  // Get the last element first
  (:= last (Vec::last v))
  (printf "\nLast element: %d\n" last)

  // Then pop to remove it
  (:=< v (Vec::pop v))
  (printf "After pop: size=%d\n" (Vec::len v))
  (printf "Remaining elements: %d, %d\n"
    (Vec::get v 0)
    (Vec::get v 1))

  (printf "\n=== Test passed! ===\n")
}
