// Simple test for Vec::push
fn test_push_one(): void {
  (:= v (Vec::new<i32>))
  (printf "Initial: size=%d, capacity=%d\n" (Vec::len<i32> v) (Vec::capacity<i32> v))
  (:=< v (Vec::push<i32> v 42))
  (printf "After push: size=%d, capacity=%d\n" (Vec::len<i32> v) (Vec::capacity<i32> v))
  (printf "v[0]=%d\n" (Vec::get<i32> v 0))
  (:=< v (Vec::push<i32> v 99))
  (printf "After push 2: size=%d, capacity=%d\n" (Vec::len<i32> v) (Vec::capacity<i32> v))
  (printf "v[0]=%d, v[1]=%d\n" (Vec::get<i32> v 0) (Vec::get<i32> v 1))
}

alloc fn main(): void {
  (test_push_one)
}
