// Test stdlib Vec operations
// Vec is now a built-in type from the standard library
// Updated to use new namespace syntax: Vec::new, Vec::get, etc.

fn test_vec_basic(): void {
  (:= v [1, 2, 3])
  (printf "Vec from array literal: size=%d, capacity=%d\n" (Vec::len v) (Vec::capacity v))
  (printf "is_empty=%d (expected 0)\n" (Vec::is_empty v))
  (printf "v[0]=%d, v[1]=%d, v[2]=%d\n" (Vec::get v 0) (Vec::get v 1) (Vec::get v 2))
  (printf "first=%d, last=%d\n" (Vec::first v) (Vec::last v))
}

fn test_vec_new(): void {
  (:= v (Vec::new<i32>))
  (printf "Vec from Vec::new: size=%d, is_empty=%d\n" (Vec::len v) (Vec::is_empty v))
}

fn test_vec_set(): void {
  (:= v [10, 20, 30])
  (printf "Before set: v[1]=%d\n" (Vec::get v 1))
  (Vec::set v 1 999)
  (printf "After set: v[1]=%d\n" (Vec::get v 1))
}

alloc fn main(): void {
  (printf "=== Test Vec Basic ===\n")
  (test_vec_basic)

  (printf "\n=== Test Vec New ===\n")
  (test_vec_new)

  (printf "\n=== Test Vec Set ===\n")
  (test_vec_set)

  (printf "\n=== All Vec tests passed! ===\n")
}
